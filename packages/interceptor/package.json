{
  "name": "cypress-interceptor",
  "version": "3.8.0",
  "description": "A helper for better Cypress testing; mocking, throttling and waiting for requests with advanced statistics",
  "devDependencies": {
    "cypress": "14.0.0"
  },
  "author": "Martin Tichovsky",
  "files": [
    "convert/*.d.ts",
    "convert/*.js",
    "*.d.ts",
    "*.js"
  ],
  "keywords": [
    "cypress",
    "intercept",
    "testing",
    "requests"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinTichovsky/cypress-interceptor"
  },
  "scripts": {
    "build": "rimraf tsconfig.build.tsbuildinfo --glob \"**/*.js\" \"**/*.d.ts\" && tsc --project tsconfig.build.json",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint .",
    "tsc": "tsc --noEmit"
  },
  "types": "interceptor.d.ts"
}
